      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ElasticSearchConnectionSettings": {
    "url": "http://localhost:9200",
    "ErrorIndex": "error_log",
    "RequestIndex": "req_log",
    "ResponseIndex": "resp_log"
  }


}
